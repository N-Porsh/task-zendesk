{
  "name": "zdt",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --runInBand --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "build": "tsc && cd client && npm run build",
    "init-db": "node dist/database/init-db.js",
    "init-db:dev": "ts-node src/database/init-db.ts",
    "setup": "bash setup.sh",
    "setup:k8s": "bash setup.sh --k8s",
    "start": "npm run init-db && concurrently \"node dist/grpc/server.js\" \"node dist/api/index.js\"",
    "dev": "npm run init-db:dev && concurrently \"ts-node src/grpc/server.ts\" \"ts-node src/api/index.ts\" \"cd client && npm run dev\"",
    "fresh": "bash setup.sh && npm run dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.8.0",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "sqlite3": "^5.1.7",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^22.0.0",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.2.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/*.test.ts"],
    "collectCoverageFrom": ["src/**/*.ts", "!src/**/*.d.ts", "!src/**/*.test.ts"],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov"],
    "setupFilesAfterEnv": ["<rootDir>/test/setup.ts"]
  }
}